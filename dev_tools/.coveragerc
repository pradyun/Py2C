[nosetests]
exe=1

[run]
branch = True

source =
    py2c

omit =
    **/py2c/tests/*

[report]
exclude_lines =
    # Changed pragma
    coverage: not missing

    # Defensive code
    raise AssertionError

    def __repr__

    # Not for testing :)
    if False:
    if __name__ == .__main__.:

partial_branches =
    # Changed pragma
    coverage: no partial

[html]
directory = test-report/coverage
